{
    "name": "awcodes/filament-sticky-header",
    "description": "A Filament Admin plugin to make headers sticky when scrolling.",
    "keywords": [
        "framework",
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Awcodes\\FilamentStickyHeader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awcodes\\FilamentStickyHeader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awcodes\\FilamentStickyHeader\\FilamentStickyHeaderServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
